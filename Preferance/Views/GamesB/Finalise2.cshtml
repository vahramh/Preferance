            </div>
            <div class="form-group">
                <label asp-for="Value" class="control-label"></label>
                <input asp-for="Value" class="form-control" />
                <span asp-validation-for="Value" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Challenge" /> @Html.DisplayNameFor(model => model.Challenge)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Contra" /> @Html.DisplayNameFor(model => model.Contra)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Kaput" /> @Html.DisplayNameFor(model => model.Kaput)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NorthSouthPoints" class="control-label"></label>
                <input asp-for="NorthSouthPoints" class="form-control" />
                <span asp-validation-for="NorthSouthPoints" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EastWestPoints" class="control-label"></label>
                <input asp-for="EastWestPoints" class="form-control" />
                <span asp-validation-for="EastWestPoints" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NorthSouthExtras" class="control-label"></label>
                <input asp-for="NorthSouthExtras" class="form-control" />
