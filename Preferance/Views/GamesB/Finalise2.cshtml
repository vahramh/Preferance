                <input asp-for="EastWestExtras" class="form-control" />
                <span asp-validation-for="EastWestExtras" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NorthSouthScore" class="control-label"></label>
                <input asp-for="NorthSouthScore" class="form-control" />
                <span asp-validation-for="NorthSouthScore" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EastWestScore" class="control-label"></label>
                <input asp-for="EastWestScore" class="form-control" />
                <span asp-validation-for="EastWestScore" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MatchBId" class="control-label"></label>
                <select asp-for="MatchBId" class="form-control" asp-items="ViewBag.MatchBId"></select>
                <span asp-validation-for="MatchBId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
