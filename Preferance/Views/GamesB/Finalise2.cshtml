@model Preferance.Models.GameB

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Finalise2</title>
</head>
<body>

<h4>GameB</h4>
<hr />
<div class="row">
